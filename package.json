{
  "name": "node+typescript+express+sequelize",
  "version": "1.0.0",
  "description": "Typescript sequelize application",
  "author": {
    "name": "VISHAL WAYACHAL",
    "email": "vishuwayachal@gmail.com"
  },
  "main": "build/src/server.js",
  "keywords": [
    "Node",
    "Express",
    "Typescript",
